{
  "id": "<%= id %>",
  "name": "<%= appname %>",
  "version": "0.0.0",
  "description": "<%= description %>",
  "author": {
    "name": "<%= author.email %>",
    "email": "<%= author.name %>"
  },
  "contributors": [
    {}
  ],
  "scripts": {
    "postinstall": "jspm install && grunt init",
    "start": "grunt fun",
    "test": "grunt test"
  },
  "repository": "<%= git %>",
  "keywords": [
    "angular",
    "cordova"
  ],
  "license": "SEE LICENSE IN LICENSE",
  "devDependencies": {
    "autoprefixer": "^6.2.3",
    "babel-core": "^6.4.0",
    "babel-eslint": "^4.1.6",
    "babel-plugin-ng-annotate": "^0.2.3",
    "babel-plugin-syntax-decorators": "^6.3.13",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "browser-sync": "^2.11.0",
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "cordova": "^5.4.1",
    "cssnano": "^3.4.0",
    "es6-module-loader": "^0.17.10",
    "grunt": "^0.4.5",
    "grunt-available-tasks": "^0.6.1",
    "grunt-browser-sync": "^2.2.0",
    "grunt-contrib-clean": "^0.7.0",
    "grunt-contrib-concat": "^0.5.1",
    "grunt-contrib-copy": "^0.8.2",
    "grunt-contrib-cssmin": "^0.14.0",
    "grunt-contrib-imagemin": "^1.0.0",
    "grunt-contrib-sass": "^0.9.2",
    "grunt-contrib-uglify": "^0.11.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-cordova-icons": "^0.1.5",
    "grunt-cordova-splashs": "^0.1.4",
    "grunt-cordovacli": "^0.8.2",
    "grunt-eslint": "^17.3.1",
    "grunt-filerev": "^2.3.1",
    "grunt-karma": "^0.12.1",
    "grunt-newer": "^1.1.1",
    "grunt-postcss": "^0.7.1",
    "grunt-processhtml": "^0.3.8",
    "grunt-sass-globbing": "^1.4.0",
    "grunt-systemjs-builder": "^0.2.5",
    "grunt-usemin": "^3.1.1",
    "imagemin-gifsicle": "^4.2.0",
    "imagemin-jpegtran": "^4.3.2",
    "imagemin-optipng": "^4.3.0",
    "imagemin-svgo": "^4.2.0",
    "jit-grunt": "^0.9.1",
    "jspm": "^0.16.19",
    "karma": "^0.13.19",
    "karma-babel-preprocessor": "^6.0.1",
    "karma-chai": "^0.1.0",
    "karma-chai-as-promised": "^0.1.2",
    "karma-chai-plugins": "^0.6.1",
    "karma-chrome-launcher": "^0.2.2",
    "karma-coverage": "^0.5.3",
    "karma-jspm": "^2.0.2",
    "karma-mocha": "^0.2.1",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sinon": "^1.0.4",
    "karma-sinon-chai": "^1.1.0",
    "karma-systemjs": "^0.10.0",
    "load-grunt-config": "^0.19.1",
    "load-grunt-tasks": "^3.4.0",
    "merge": "^1.2.0",
    "mocha": "^2.3.4",
    "nightwatch": "^0.8.15",
    "phantomjs": "^1.9.19",
    "sinon": "^1.17.2",
    "systemjs": "^0.19.15",
    "time-grunt": "^1.3.0"
  },
  "engines": {
    "node": ">=5.3.0"
  },
  "jspm": {
    "configFile": "jspm.config.js",
    "dependencies": {
      <% if (isAngular) { %>
      "angular": "npm:angular@1.5.0-rc.1",
      "angular-animate": "npm:angular-animate@1.5.0-rc.1",
      "angular-sanitize": "npm:angular-sanitize@1.5.0-rc.1",
      "angular-ui-router": "github:angular-ui/ui-router@^0.2.15",
      "ionic": "github:driftyco/ionic-bower@^1.2.4",
      <% } %>
      <% if (isAngular2) { %>
      "angular2": "npm:angular2@^2.0.0-beta.0",
      <% } %>
      "css": "github:systemjs/plugin-css@^0.1.20",
      "json": "github:systemjs/plugin-json@^0.1.0",
      "process": "github:jspm/nodelibs-process@^0.1.2",
      "text": "github:systemjs/plugin-text@^0.0.4"
    },
    "devDependencies": {
      "babel": "npm:babel-core@^5.8.24",
      "babel-runtime": "npm:babel-runtime@^5.8.24",
      "core-js": "npm:core-js@^1.1.4",
      "clean-css": "npm:clean-css@^3.4.9"
    },
    "overrides": {
      <% if (isAngular) { %>
      "github:driftyco/ionic-bower@1.2.4": {
        "registry": "jspm",
        "main": "js/ionic-angular",
        "shim": {
          "js/ionic-angular": {
            "exports": "ionic",
            "deps": [
              "./ionic",
              "angular",
              "angular-animate",
              "angular-sanitize",
              "angular-ui-router"
            ]
          }
        },
        "dependencies": {
          "css": "^0.1.5",
          "angular": "^1.5.0-rc.1",
          "angular-animate": "^1.5.0-rc.1",
          "angular-sanitize": "^1.5.0-rc.1",
          "angular-ui-router": "0.2.15"
        }
      },
      "github:angular-ui/ui-router@0.2.15": {
        "registry": "jspm",
        "dependencies": {
          "angular": "^1.5.0-rc.1"
        }
      }
      <% } %>
    }
  }
}
